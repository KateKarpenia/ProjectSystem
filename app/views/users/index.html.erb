<h1>Listing Users</h1>

<table>

	<tr>
		<th>First Name</th>
		<th>Last Name</th>
		<th>Email</th>
		<th>Admin</th>
	</tr>

<% @user.each do |user| %>
		<tr>
			<td><%= user.first_name %></td>
			<td><%= user.last_name %></td>
			<td><%= user.email %></td>
			<td><%= user.admin %></td>
		</tr>
		
	<% end %>

        
</table>

