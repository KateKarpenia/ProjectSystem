<h1>Listing projects</h1>

<%= link_to 'New project', new_project_path %>

<table>
	<tr>
		<th>Name</th>
		<th>Summary</th>
		<th>Start Date</th>
		<th>End Date</th>
	</tr>
	
	<% @project.each do |project| %>
		<tr>
			<td><%= project.name %></td>
			<td><%= project.summary %></td>
			<td><%= project.start_date %></td>
			<td><%= project.end_date %></td>
		</tr>
	<% end %>
</table>

<%= link_to 'Back', projects_path %>
